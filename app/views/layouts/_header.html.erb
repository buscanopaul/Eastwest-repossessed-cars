<br>
<header>
    <div class="container">
        <div class="row">
            <% if user_signed_in? %>
                <%= link_to "Edit Profile", edit_user_registration_path, class: "btn btn-outline-light" %>
                <%= link_to "Logout", destroy_user_session_path, method: :delete, class: "btn btn-outline-light" %>
                <%= link_to "Brands", brands_path, class: "btn btn-outline-light" %>
                <%= link_to "Cars", cars_path, class: "btn btn-outline-light" %>
                <%= link_to "Types", types_path, class: "btn btn-outline-light" %>
                <%= link_to "Offers", offers_path, class: "btn btn-outline-light" %>
                <% if current_user.issuperadmin? %>
                     <%= link_to "Users", users_path, class: "nav-link px-2 text-white" %>
                <% end %>
            <% else %>
                <%= link_to "Log in", new_user_session_path, class: "btn btn-outline-light" %>
                <%= link_to "Sign up", new_user_registration_path, class: "btn btn-outline-light" %>
            <% end %>
        </div>
    </div>
</header>